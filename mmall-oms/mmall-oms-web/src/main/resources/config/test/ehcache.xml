<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
	<!-- DefaultCache setting. -->
	<defaultCache maxBytesLocalHeap="30M"
		memoryStoreEvictionPolicy="LRU" eternal="false" timeToIdleSeconds="300"
		timeToLiveSeconds="300" overflowToDisk="false" diskPersistent="false" />

	<!-- Special objects setting. -->
	<cache name="expressFeeCache" maxBytesLocalHeap="30M"
		memoryStoreEvictionPolicy="LRU" eternal="false" timeToIdleSeconds="3600"
		timeToLiveSeconds="3600" overflowToDisk="false" diskPersistent="false" />
	<cache name="warehouseFormCache" maxBytesLocalHeap="30M"
		memoryStoreEvictionPolicy="LRU" eternal="false" timeToIdleSeconds="300"
		timeToLiveSeconds="3600" overflowToDisk="false" diskPersistent="false" />	
	<cache name="omsFreightTempletCache" maxBytesLocalHeap="30M"
		memoryStoreEvictionPolicy="LRU" eternal="false" timeToIdleSeconds="300"
		timeToLiveSeconds="3600" overflowToDisk="false" diskPersistent="false" />
	<cache name="omsEducationCache" maxBytesLocalHeap="30M"
		memoryStoreEvictionPolicy="LRU" eternal="false" timeToIdleSeconds="300"
		timeToLiveSeconds="3600" overflowToDisk="false" diskPersistent="false" />	
	<cache name="omsRegionCache" maxBytesLocalHeap="30M"
		memoryStoreEvictionPolicy="LRU" eternal="false" timeToIdleSeconds="300"
		timeToLiveSeconds="3600" overflowToDisk="false" diskPersistent="false" />
</ehcache>

<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
	<!-- DefaultCache setting. -->
	<defaultCache maxBytesLocalHeap="50M"
		memoryStoreEvictionPolicy="LRU" eternal="false" timeToIdleSeconds="300"
		timeToLiveSeconds="300" overflowToDisk="false" diskPersistent="false" />

	<!-- Special objects setting. -->
	<cache name="helpContentCategory" maxBytesLocalHeap="10M"
		memoryStoreEvictionPolicy="LRU" eternal="false" timeToIdleSeconds="300"
		timeToLiveSeconds="1200" overflowToDisk="false" diskPersistent="false" />
	<cache name="publishedArticle" maxBytesLocalHeap="50M"
		memoryStoreEvictionPolicy="LRU" eternal="false" timeToIdleSeconds="300"
		timeToLiveSeconds="1200" overflowToDisk="false" diskPersistent="false" />
	<cache name="presentProductList" maxBytesLocalHeap="50M"
		memoryStoreEvictionPolicy="LRU" eternal="false" timeToIdleSeconds="1800"
		timeToLiveSeconds="3600" overflowToDisk="false" diskPersistent="false" />	
	<cache name="latestLotteryGiftList" maxBytesLocalHeap="50M"
		memoryStoreEvictionPolicy="LRU" eternal="false" timeToIdleSeconds="60"
		timeToLiveSeconds="180" overflowToDisk="false" diskPersistent="false" />
	<cache name="categoryContentList" maxBytesLocalHeap="100M"
		memoryStoreEvictionPolicy="LRU" eternal="false" timeToIdleSeconds="1800"
		timeToLiveSeconds="1800" overflowToDisk="false" diskPersistent="false" />	
	<cache name="categoryContentMap" maxBytesLocalHeap="100M"
		memoryStoreEvictionPolicy="LRU" eternal="false" timeToIdleSeconds="1800"
		timeToLiveSeconds="1800" overflowToDisk="false" diskPersistent="false" />
</ehcache>

	font-size: 14px;
	color: #fff;
	text-align: right;
}

/*商品模块*/
.bwd-listCont {
	padding: 63px 0;
	background-color: #f7e7c1;
}
.bwd-module {
	width: 1190px;
	margin: 0 auto;
	background-color: #f7e7c1;
	overflow: hidden;
}
.bwd-h3 {
	position: relative;
	background-color: #f4ad03;
	font-size: 40px;
	color: #fff;
	line-height: 72px;
	text-align: center;
	overflow: hidden;
	border-radius: 30px 30px 0 0;
}
.bwd-more {
	font-weight: normal;
	position: absolute;
	right: 30px;
	padding: 0 20px;
	font-size: 20px;
	color: #452701;
	text-decoration: none;
	z-index: 3;
}

.bwd-items {
	position: relative;
	width: 1020px;
	padding-left: 191px;
}
.bwd-itemBanner {
	position: absolute;
	left: 0;
	top: 0;
}
/*item*/
.bwd-item {
	position: relative;
	float: left;
	width: 238px;
	height: 318px;
	margin: 0 13px 13px 0;
	background-color: #fff;
	border: 1px solid #fff;
	overflow: hidden;
}
.bwd-item:hover {

/* 顶部导航 */
.m-topnav3-box{
	width:100%;
	background:$fc0;
}
.m-topnav3{
	width:1090px;
	margin:0 auto;
	height:28px;
	.bar {
		float:right;padding:0;height:18px;line-height:26px;
		.u-tag1{margin-left:2px;}
	}
	.bar li {
		float: left;
		&.sep{
			width:0;height: 12px;margin:3px 16px 0;overflow:hidden; border-left: 1px solid #dedede;position:relative;top:4px;
		}
		&.item{
			position: relative;z-index:99;
			.menu{
				$inline-block();position:relative;padding-right:12px;
				i{position:absolute;right:0;top:50%;margin-top:-2px;}
			}
			.u-menu{margin:0 0 0 -50px;}
			.u-menu2{margin:0 0 0 -50px;}
			.cart{
				.u-iconf{padding-right:5px;}
			}
			.u-icon-arrow1{
				margin-left:5px;$transition:transform 0.2s ease-in;
			}
			&:hover{
			  .u-icon-arrow1{$transform:rotate(180deg);}
        	  .u-menu{display:block;}
        	  .u-menu2{display:block;}
			}
			&-active{
				.u-icon-arrow1{$transform:rotate(180deg);}
				.u-menu{display:block;}
				.u-menu2{display:block;}
			}
		}
	}
	.logo{
		float:left;padding:26px 0 0;
		margin-right:4px;
		a{
			display:block;width:130px;height:58px;overflow:hidden;text-indent:-2000px;@extend bg-logo;background-position:0 0;
		}

.m-split{height: 2px;width: 100%; overflow: hidden;background-color: #1e1e20;}
/* 主导航 */
.m-nav {
	font-size:16px;background:#1e1e20;
	.nav{
		position:relative;@extend clearfix;
		li{
			position:relative;z-index:2;float: left;width: 130px; height: 40px; line-height: 40px;
			a{
				display: block; padding: 0 5px; text-align: center;
				&,&:hover{color:#fff;}
			}
			&.last{
				position: absolute; display: block; z-index:1; left: -120px; top: 0;background: #2f2f31;
				-moz-transition: -moz-transform 0.3s; -o-transition: -o-transform 0.3s; -webkit-transition: -webkit-transform 0.3s; transition: transform 0.3s;
			}
			&.selected{
				&:nth-child(1) ~ li.last {$transform: translateX(120px);}
				&:nth-child(2) ~ li.last {$transform: translateX(250px); }
				&:nth-child(3) ~ li.last {$transform: translateX(380px); }
				&:nth-child(4) ~ li.last {$transform: translateX(510px); }
			}
			&:nth-child(1):hover ~ li.last {$transform: translateX(120px);}
			&:nth-child(2):hover ~ li.last {$transform: translateX(250px); }
			&:nth-child(3):hover ~ li.last {$transform: translateX(380px); }
			&:nth-child(4):hover ~ li.last {$transform: translateX(510px); }
		}
	}

}
/* ie fix */
.m-nav{
	.nav{
		li{
			&.last{display: none \9;}
			&.selected,a:hover {background: #2f2f31 \9;}
		}
	}
}
:root .m-nav{
	.nav{
		li{
			&.last{display: block;}
			&.selected,a:hover {background: none;}
		}
	}
}

.m-tab{
	position:relative;z-index:999;
	.tab{
		@extend clearfix;
		li{
			position:relative;float: left; width:80px; height: 40px; line-height: 40px; color: #fff; text-align: center; font-size: 14px; cursor: pointer;
			i{
				display:none;position: absolute; top: 100%; left: 50%; margin-left:-5px; width: 0; height: 0;overflow:hidden; border-width: 5px; border-style: solid dashed dashed dashed; border-color: $fc42 transparent transparent;
			}
			a{
				position:relative;display:block;zoom:1;margin-right:1px;color:#fff;background: #4d4e53;
				&:hover{color:#fff;}
			}
			&.last{
				margin-right:-10px;
				a{margin-right:0;}
			}
			&.z-active{
				a{background:$fc42;}
				i{display:block;}
			}
		}
	}
}
/* 个人中心 我的关注 tab */
.m-tab1{
	height:52px;background:#eaeaea;
	ul{@extend clearfix;}
	li{
		float:left; width:152px; height:52px;line-height:52px;font-size:14px;text-align:center;cursor:pointer;
		a{
			display:block;
			&,&:hover{color:#666;}
		}
		&.active{
			a,a:hover{color:#333;font-weight:bold;background:#fff;cursor:default;}
		}
	}
}
/* 帮助中心 新手引导 tab */
.m-tab1-1{

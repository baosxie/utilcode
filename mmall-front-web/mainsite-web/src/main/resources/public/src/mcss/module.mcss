// 动画
$fadeIn();
$fadeInX();
$fadeOut();
$bounceOut();

.animated{
  $animation-duration: .6s;
}
.animated.fast{
  $animation-duration: .3s;
}
@keyframes nodeInserted {
  from { opacity: 0; }
  to { opacity: 0.4; }
}
@-webkit-keyframes nodeInserted {
  from { opacity: 0; }
  to { opacity: 0.4; }
}
@-moz-keyframes nodeInserted {
  from { opacity: 0; }
  to { opacity: 0.4; }
}
@-o-keyframes nodeInserted {
  from { opacity: 0; }
  to { opacity: 0.4; }
}
.m-mask{
	position: fixed;
    z-index: 3000!important;
	top:0;
	left: 0;
	right: 0;
	bottom: 0;
  	background:#000;
  	$opacity:0.4;
	$animation-duration: 0.3s;
	$animation-name: nodeInserted;
}

.m-modal{
  overflow-y: auto;
  overflow-x: hidden;
  text-align: center;
  position: fixed;
  top:0;
  left:0;
  right: 0;
  bottom: 0px;
  vertical-align: middle;
  white-space: nowrap;
  z-index:5000;
  background-color: rgba(#000, 0.6);
}
.m-modal .container{
  vertical-align: middle;
  word-wrap: break-word;
  white-space: normal;
  display: inline-block;
  zoom:1;
}
.m-modal .placeholder{
  width:1px;
  height: 100%;
  zoom: 1;
  margin-right: -0.31em;
  overflow: hidden;
  display: inline-block;
  vertical-align: middle;
  zoom:1;
}


/* product-list */
.from-group{padding-bottom: 50px; }
.list-page-up ,.list-page-down {float: right;}
/* po-alist */
.form-group .sep{position:absolute;left:-7px;top:7px;}
/* po-add */
.control-buadd{text-align: left;padding-top: 7px;}
.buadd-id{color: #cc0000;font-size: 16px;font-weight: 700;padding-bottom: 20px;}

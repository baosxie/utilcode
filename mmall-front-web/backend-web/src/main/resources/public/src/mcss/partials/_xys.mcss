.addlist-hd{clear: both;margin-top: 40px;overflow:hidden; margin-bottom: -1px}
.addlist-hd ul{width: 356px;height: 40px;padding:0px;margin: 0px; border: 1px solid #bbb;border-width: 1px 0 0 1px;}
.addlist-hd ul li{font-size: 16px;float: left; list-style: none;width: 177px;height: 40px;line-height: 40px;cursor: pointer;text-align: center;border: 1px solid #bbb;border-width: 0 1px 0px 0;background-color: #f7f7f7;}
.addlist-hd .addclick{font-weight: 700;border-bottom: 1px solid #fff; color: #2b2b2b;background-color: #fff;}

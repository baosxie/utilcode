<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
        <!-- DefaultCache setting. -->
        <defaultCache maxBytesLocalHeap="50M"
                memoryStoreEvictionPolicy="LRU" eternal="false" timeToIdleSeconds="600"
                timeToLiveSeconds="600" overflowToDisk="false" diskPersistent="false" />

        <!-- Special objects setting. -->
        <cache name= "poCache"  maxBytesLocalHeap="100M"
                memoryStoreEvictionPolicy="LRU" eternal="false" timeToIdleSeconds="600"
                timeToLiveSeconds="600" overflowToDisk="false" diskPersistent="false" />
        
        <cache name= "brandCache"  maxBytesLocalHeap="50M"
                memoryStoreEvictionPolicy="LRU" eternal="false" timeToIdleSeconds="600"
                timeToLiveSeconds="600" overflowToDisk="false" diskPersistent="false" />
   </ehcache>
